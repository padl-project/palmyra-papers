	keywords = {Biodiversity, Animal behaviour, padl260, Zoology},
	pages = {409},
	file = {Full Text PDF:/Users/camilavargas/Zotero/storage/UXBFP8WT/McCauley et al. - 2012 - From wing to wing\: the persistence of long ecologi.pdf:application/pdf;Snapshot:/Users/camilavargas/Zotero/storage/28DKJCV7/McCauley et al. - 2012 - From wing to wing\: the persistence of long ecologi.html:text/html},
}

