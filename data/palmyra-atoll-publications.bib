	keywords = {Ecology, Evolutionary Biology, general, Life Sciences, Microbial Ecology, Microbial Genetics and Genomics, Microbiology, padl262},
	pages = {638--649},
	file = {Full Text PDF:/Users/camilavargas/Zotero/storage/DKQCZ5UF/Kelly et al. - 2012 - Black reefs\: iron-induced phase shifts on coral re.pdf:application/pdf;Snapshot:/Users/camilavargas/Zotero/storage/X8SQ9VPZ/Kelly et al. - 2012 - Black reefs\: iron-induced phase shifts on coral re.html:text/html},
