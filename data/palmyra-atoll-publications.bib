	keywords = {padl68},
	pages = {493--509},
	file = {Full Text PDF:/Users/camilavargas/Zotero/storage/VCEUE4GA/Vidal-Martínez et al. - 2012 - Digenean metacercariae of fishes from the lagoon f.pdf:application/pdf;Snapshot:/Users/camilavargas/Zotero/storage/35IFZGB5/Vidal-Martínez et al. - 2012 - Digenean metacercariae of fishes from the lagoon f.html:text/html},
