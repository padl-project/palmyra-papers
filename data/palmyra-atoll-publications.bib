@article{lafferty_stomach_2010,
	title = {Stomach {Nematodes} ({Mastophorus} muris) in {Rats} ({Rattus} rattus) {Are} {Associated} with {Coconut} ({Cocos} nucifera) {Habitat} at {Palmyra} {Atoll}},
	volume = {96},
	issn = {0022-3395, 1937-2345},
	url = {https://bioone.org/journals/journal-of-parasitology/volume-96/issue-1/GE-2180.1/Stomach-Nematodes-Mastophorus-muris-in-Rats-Rattus-rattus-Are-Associated/10.1645/GE-2180.1.full},
	doi = {10.1645/GE-2180.1},
	abstract = {Black rats (Rattus rattus) and their stomach nematodes (Mastophorus muris) were historically introduced to islets at Palmyra Atoll in the central Pacific Line Islands. To investigate patterns of parasitism, we trapped rats and quantified nematodes on 13 islets of various sizes and habitat types. Most rats were parasitized (59\%) with an average of 12 worms per infected rat. Islet size did not greatly influence parasite population biology. Nematodes also did not appear to affect rat condition (weight to skull length). The only strong and consistent factor associated with the mean abundance of nematodes in rats was habitat (dominant cover and locally dominant plant species). Thus, nematodes were much more abundant in rats from sites dominated by coconut trees (Cocos nucifera). Coconut trees may also be an introduced species at Palmyra Atoll.},
	number = {1},
	urldate = {2022-05-20},
	journal = {Journal of Parasitology},
	author = {Lafferty, Kevin D. and Hathaway, Stacie A. and Wegmann, Alex S. and Shipley, Frank S. and Backlin, Adam R. and Helm, Joel and Fisher, Robert N.},
	month = feb,
	year = {2010},
