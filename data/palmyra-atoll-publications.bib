	author = {Nakano, Takafumi and Suzuki, Hajime and Suzuki, Naoko and Kimura, Yuichi and Sato, Tatsuo and Kamigaichi, Hiromi and Tomita, Naoki and Yamasaki, Takeshi},
	month = dec,
