	journal = {ZooKeys},
	author = {Vidal-Martinez, Victor Manuel and Soler-Jimenez, Lilia Catherinne and Aguirre-Macedo, Ma Leopoldina and Mclaughlin, John and Jaramillo, Alejandra and Shaw, Jenny and James, Anna and Hechinger, Ryan and Kuris, Armand and Lafferty, Kevin},
