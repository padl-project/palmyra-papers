@article{ WOS:000280787400021,
Author = {Tidgewell, Kevin and Engene, Niclas and Byrum, Tara and Media, Joseph
   and Doi, Takayuki and Valeriote, Fred A. and Gerwick, William H.},
Title = {Evolved Diversification of a Modular Natural Product Pathway: Apratoxins
   F and G, Two Cytotoxic Cyclic Depsipeptides from a Palmyra Collection of
   Lyngbya bouillonii},
Journal = {CHEMBIOCHEM},
