ISSN = {2041-210X},
Keywords = {acoustic telemetry; detection probability; Ornstein-Uhlenbeck process;
   state-space model},
Keywords-Plus = {MARK-RECAPTURE DATA; ACOUSTIC TRACKING; MODELS; TELEMETRY; BEHAVIOR;
