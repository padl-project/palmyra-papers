@article{ WOS:000547821500001,
Author = {Neal, B. P. and Honisch, B. and Warrender, T. and Williams, G. J. and
   Work, T. M. and Price, N. N.},
Title = {Possible control of acute outbreaks of a marine fungal pathogen by
   nominally herbivorous tropical reef fish},
Journal = {OECOLOGIA},
Year = {2020},
Volume = {193},
Number = {3},
Pages = {603-617},
Month = {JUL},
Abstract = {Primary producers in terrestrial and marine systems can be affected by
   fungal pathogens threatening the provision of critical ecosystem
   services. Crustose coralline algae (CCA) are ecologically important
   members of tropical reef systems and are impacted by coralline fungal
   disease (CFD) which manifests as overgrowth of the CCA crust by fungal
   lesions causing partial to complete mortality of the CCA host. No
   natural controls for CFD have been identified, but nominally herbivorous
   fish could play a role by consuming pathogenic fungi. We documented
   preferential grazing on fungal lesions by adults of six common
   reef-dwelling species of herbivorous Acanthuridae and Labridae,
   (surgeonfish and parrotfish) which collectively demonstrated an similar
   to 80-fold higher grazing rate on fungal lesions relative to their
   proportionate benthic coverage, and a preference for lesions over other
   palatable substrata (e.g. live scleractinian coral, CCA, or algae).
   Furthermore, we recorded a similar to 600\% increase in live CFD lesion
   size over an approximately 2-week period when grazing by herbivorous
   fish was experimentally excluded suggesting that herbivorous reef fish
   could control CFD progression by directly reducing biomass of the fungal
   pathogen. Removal rates may be sufficient to allow CCA to recover from
   infection and explain historically observed natural waning behaviour
   after an outbreak. Thus, in addition to their well-known role as
   determinants of macroalgal overgrowth of reefs, herbivorous fish could
   thus also be important in control of diseases affecting crustose
   coralline algae that stabilize the foundation of coral reef substrata.},
