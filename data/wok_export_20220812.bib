@article{ WOS:000561670100001,
Author = {Papastamatiou, Yannis P. and Bodey, Thomas W. and Caselle, Jennifer E.
   and Bradley, Darcy and Freeman, Robin and Friedlander, Alan M. and
   Jacoby, David M. P.},
Title = {Multiyear social stability and social information use in reef sharks
   with diel fission-fusion dynamics},
Journal = {PROCEEDINGS OF THE ROYAL SOCIETY B-BIOLOGICAL SCIENCES},
Year = {2020},
Volume = {287},
Number = {1932},
Month = {AUG 12},
Abstract = {Animals across vertebrate taxa form social communities and often exist
   as fission-fusion groups. Central place foragers (CPF) may form groups
   from which they will predictably disperse to forage, either individually
   or in smaller groups, before returning to fuse with the larger group.
   However, the function and stability of social associations in predatory
   fish acting as CPFs is unknown, as individuals do not need to return to
   a shelter yet show fidelity to core areas. Using dynamic social networks
   generated from acoustic tracking data, we document spatially structured
   sociality in CPF grey reef sharks at a Pacific Ocean atoll. We show that
   sharks form stable social groups over multiyear periods, with some
   dyadic associations consistent for up to 4 years. Groups primarily
   formed during the day, increasing in size throughout the morning before
   sharks dispersed from the reef at night. Our simulations suggest that
   multiple individuals sharing a central place and using social
   information while foraging (i.e. local enhancement) will outperform
   non-CPF social foragers. We show multiyear social stability in sharks
   and suggest that social foraging with information transfer could provide
   a generalizable mechanism for the emergence of sociality with group
   central place foraging.},
