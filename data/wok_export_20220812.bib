@article{ WOS:000278626500053,
Author = {Baumann-Pickering, Simone and Wiggins, Sean M. and Roth, Ethan H. and
   Roch, Marie A. and Schnitzler, Hans-Ulrich and Hildebrand, John A.},
Title = {Echolocation signals of a beaked whale at Palmyra Atoll},
