   Dunbar, Robert B., Stanford Univ, Dept Environm Earth Syst Sci, Stanford, CA 94305 USA.
   Garton, David W., Georgia Inst Technol, Sch Biol, Atlanta, GA 30332 USA.},
DOI = {10.1016/j.marpolbul.2010.11.004},
ISSN = {0025-326X},
