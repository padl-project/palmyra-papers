   Caselle, Jennifer E., Univ Calif Santa Barbara, Inst Marine Sci, Santa Barbara, CA 93106 USA.
   Lowe, Christopher G., Calif State Univ Long Beach, Dept Biol Sci, Long Beach, CA 90840 USA.},
DOI = {10.1016/j.jembe.2010.02.009},
